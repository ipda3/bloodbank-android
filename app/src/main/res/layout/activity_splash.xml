<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gfgfg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/splash"
    tools:context=".ui.activity.startUpCycle.SplashActivity">

    <ImageView
        android:id="@+id/Splash_Iv_Logo"
        android:layout_width="@dimen/dimens_splash_img"
        android:layout_height="@dimen/dimens_splash_img"
        android:layout_centerInParent="true"
        android:src="@drawable/logo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/dim_m"
        android:text="@string/lows"
        android:textSize="@dimen/txt_size_s" />

</RelativeLayout>